{
  "name": "thewebsolver/cli",
  "description": "CLI for TheWebSolver libraries",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Shesh Ghimire",
      "email": "sheshgh@outlook.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "symfony/console": "^7.2",
    "symfony/event-dispatcher": "^7.2",
    "thewebsolver/code-generator": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "TheWebSolver\\Codegarage\\Cli\\": "Src/"
    }
  },
	"autoload-dev": {
    "psr-4": {
      "TheWebSolver\\Codegarage\\": "tests/PHPUnit/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../code-generator",
      "options": {
        "symlink": false
      }
    }
  ],
  "scripts": {
		"test": "vendor/bin/phpunit && phpstan analyze"
	},
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "phpstan/phpstan": "^2.1"
  }
}
